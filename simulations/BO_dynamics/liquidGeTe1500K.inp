&GLOBAL  
    PROJECT results/liquid_preparation
    RUN_TYPE MD
    PRINT_LEVEL MEDIUM #Â LOW, MEDIUM work - HIGH, DEBUG crash code
    SEED 123456
    WALLTIME 23:50:00
    PREFERRED_FFT_LIBRARY FFTSG
&END GLOBAL

&EXT_RESTART 
    RESTART_FILE_NAME results/liquid_preparation-1.restart
&END EXT_RESTART

&ATOM
    &METHOD
        METHOD_TYPE KOHN-SHAM
    &END METHOD    
&END ATOM

&MOTION
    &MD
        ENSEMBLE NVT
	STEPS 20000
        MAX_STEPS 20000
	TIMESTEP 2.0 #fs
	TEMPERATURE 1500
	COMVEL_TOL 1E-12 # maximum accepted velocity of center of mass
	&THERMOSTAT
	    TYPE AD_LANGEVIN
	    &AD_LANGEVIN
                TIMECON_NH 20.0 #fs, Nose-Hoover part of the thermostat
		TIMECON_LANGEVIN 20.0 #fs Langevin part of the thermostat
	    &END AD_LANGEVIN
	&END THERMOSTAT
	&PRINT
	    FORCE_LAST T # Print output and restart file if walltime is reached
            &ENERGY
                &EACH
                    MD 1
		&END EACH
	    &END ENERGY
            &PROGRAM_RUN_INFO
                &EACH
                    MD 10
		&END EACH
	    &END PROGRAM_RUN_INFO
	&END PRINT
	&AVERAGES
	    &PRINT_AVERAGES ON
                &EACH
		    MD 100
		&END EACH
	    &END PRINT_AVERAGES
	&END AVERAGES
    &END MD
    &PRINT
        &RESTART
	    ADD_LAST NUMERIC
	    &EACH
	        MD 1
	    &END EACH
	&END RESTART
        &RESTART_HISTORY
            &EACH 
	        MD 100
            &END EACH
	&END RESTART_HISTORY
	&CELL
	    &EACH
	        MD 1
	    &END EACH
	&END CELL
        &FORCES
            &EACH
                MD 1
            &END EACH
            # UNIT eV*angstrom^-1   
        &END FORCES
        &STRESS OFF
        &END STRESS
        &TRAJECTORY
            &EACH
                MD 1
            &END EACH
            # UNIT angstrom
        &END TRAJECTORY
        &VELOCITIES ON
            &EACH
                MD 1
            &END EACH
            # UNIT angstrom*ps^-1 
        &END VELOCITIES
    &END PRINT
&END MOTION

&FORCE_EVAL
    METHOD QUICKSTEP
    STRESS_TENSOR NONE
    &DFT
        MULTIPLICITY 1
        BASIS_SET_FILE_NAME ./basis_set_tzvp
        POTENTIAL_FILE_NAME ./GTH_POTENTIAL_SCAN
        &MGRID
            NGRIDS 4      # CONVERGENCE PARAMETER - Elham used:   5 - optimized in BASIS_MOLOPT
            CUTOFF 100     # CONVERGENCE PARAMETER - Elham used: 300
            REL_CUTOFF 30  # CONVERGENCE PARAMETER - Elham used:  50
        &END MGRID
        &POISSON
	    PERIODIC XYZ
        &END POISSON
        &QS # Settings of the Quickstep algorithm         
            METHOD GAPW
            EXTRAPOLATION ASPC
            EXTRAPOLATION_ORDER 3
            EPS_DEFAULT 1.0E-12
            EPS_PGF_ORB 1.0E-8
        &END QS
	&SCF
            SCF_GUESS RESTART # Use the RESTART file as an initial guess (and ATOMIC if not present).       
	    EPS_SCF 1.0E-6 
	    MAX_SCF 200
	    &OT T
	        ENERGY_GAP 0.001
		MINIMIZER CG
	    &END OT
	    &OUTER_SCF
                EPS_SCF 1.0E-6
		MAX_SCF 200
	    &END OUTER_SCF
	&END SCF
        &XC
            &XC_FUNCTIONAL
	        &MGGA_X_SCAN
	        &END MGGA_X_SCAN
	        &MGGA_C_SCAN
	        &END MGGA_C_SCAN
            &END XC_FUNCTIONAL
        &END XC 
    &END DFT
    &PRINT
        &FORCES ON
            NDIGITS 8
            &EACH
                MD 1
            &END EACH
        &END FORCES
        &STRESS_TENSOR OFF
        &END STRESS_TENSOR
    &END PRINT
    &SUBSYS
        &PRINT
            &CELL
                &EACH
                    MD 10
                &END EACH
            &END CELL
        &END PRINT
        &CELL
            PERIODIC XYZ
	    A 14.800000 0.000000 0.000000
	    B 0.000000 14.800000 0.000000
	    C 0.000000 0.000000 14.800000 
        &END CELL
